<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
		<section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=4.4.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
		<section name="reportSection" type="ExcelLoadTestReport.ReportDefaults, ExcelLoadTestReport" requirePermission="false" />
	</configSections>
	<appSettings>
		<add key="IsDebug" value="true"/>
	</appSettings>
	<connectionStrings>
		<add name="LoadTest2010" connectionString="metadata=res://*/Models.LoadTest2010.csdl|res://*/Models.LoadTest2010.ssdl|res://*/Models.LoadTest2010.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=TPHRNA01;initial catalog=LoadTest2010;integrated security=True;multipleactiveresultsets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
		<add name="LoadTest2010Entities" connectionString="metadata=res://*/Models.LoadTest2010.csdl|res://*/Models.LoadTest2010.ssdl|res://*/Models.LoadTest2010.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=TPHRNA01;initial catalog=LoadTest2010;integrated security=True;multipleactiveresultsets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
	</connectionStrings>
	<entityFramework>
		<defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework">
			<parameters>
				<parameter value="v11.0" />
			</parameters>
		</defaultConnectionFactory>
	</entityFramework>
	<reportSection>
		<reportTypes>
			<reportType type="ExcelLoadTestReport.PageTemplates.PivotTableAndChartTemplate, ExcelLoadTestReport" name ="PivotTableAndCharts"/>
			<reportType type="ExcelLoadTestReport.PageTemplates.StatisticalTables, ExcelLoadTestReport" name ="StatsTables"/>
		</reportTypes>
		<reports>
			
			<!-- This section covers the statistics-->
			<report reportName="Transaction Response Time" reportTypeName="StatsTables">
				<counters>
					 <!--Be sure to include the filterOutLoadTestRig="false" attribute otherwise these will not show up--> 
					<counter counterCategory="LoadTest:Transaction" counterName="Avg. Response Time" filterOutLoadTestRig="false" />
				</counters>
			</report>
			<report reportName="Page Response Time" reportTypeName="StatsTables">
				<counters>
					 <!--Be sure to include the filterOutLoadTestRig="false" attribute otherwise these will not show up--> 
					<counter counterCategory="LoadTest:Page" counterName="Avg. Page Time" filterOutLoadTestRig="false" />
				</counters>
			</report>
			<!-- Leaving this particular item out as it requires a lot of work on the Excel side and can error out the results.-->
			<!--<report reportName="Request Response Time" reportTypeName="StatsTables">
				<counters>
					 --><!--Be sure to include the filterOutLoadTestRig="false" attribute otherwise these will not show up--><!-- 
					<counter counterCategory="LoadTest:Request" counterName="Avg. Response Time" filterOutLoadTestRig="false" />
				</counters>
			</report>-->
			
			<!-- Windows performance counter charts-->
			<report reportName="CPU" reportTypeName="PivotTableAndCharts">
				<counters>
					<!-- Windows performance counters-->
					<counter counterCategory="Processor" counterName="% Processor Time" />
				</counters>
			</report>
			<report reportName="Memory" reportTypeName="PivotTableAndCharts">
				<counters>
					<!-- Windows performance counters-->
					<counter counterCategory="Memory" counterName="Available MBytes" />
				</counters>
			</report>
			<report reportName="Process Processor Time" reportTypeName="PivotTableAndCharts">
				<counters>
					<!-- Windows performance counters-->
					<counter counterCategory="Process" counterName="% Processor Time" />
				</counters>
			</report>
			<report reportName="Private Bytes" reportTypeName="PivotTableAndCharts">
				<counters>
					<!-- Windows performance counters-->
					<counter counterCategory="Process" counterName="Private Bytes" />
				</counters>
			</report>
			<report reportName="Physical Disk" reportTypeName="PivotTableAndCharts">
				<counters>
					<!-- Windows performance counters-->
					<counter counterCategory="PhysicalDisk" />
				</counters>
			</report>
			<report reportName="Logical Disk" reportTypeName="PivotTableAndCharts">
				<counters>
					<!-- Windows performance counters-->
					<counter counterCategory="LogicalDisk" />
				</counters>
			</report>
			<report reportName="Transaction Response Time" reportTypeName="PivotTableAndCharts">
				<counters>
					<!-- Be sure to include the filterOutLoadTestRig="false" attribute otherwise these will not show up -->
					<counter counterCategory="LoadTest:Transaction" counterName="Avg. Response Time" filterOutLoadTestRig="false" />
				</counters>
			</report>
			<report reportName="Transactions Per Second" reportTypeName="PivotTableAndCharts">
				<counters>
					<!-- Be sure to include the filterOutLoadTestRig="false" attribute otherwise these will not show up -->
					<counter counterCategory="LoadTest:Transaction" counterName="Transactions/Sec" filterOutLoadTestRig="false" />
				</counters>
			</report>
			<report reportName="Page Response Time" reportTypeName="PivotTableAndCharts">
				<counters>
					<!-- Be sure to include the filterOutLoadTestRig="false" attribute otherwise these will not show up -->
					<counter counterCategory="LoadTest:Page" counterName="Avg. Page Time" filterOutLoadTestRig="false" />
				</counters>
			</report>
			<report reportName="Request Response Time" reportTypeName="PivotTableAndCharts">
				<counters>
					<!-- Be sure to include the filterOutLoadTestRig="false" attribute otherwise these will not show up -->
					<counter counterCategory="LoadTest:Request" counterName="Avg. Response Time" filterOutLoadTestRig="false" />
				</counters>
			</report>
			
		</reports>
	</reportSection>
</configuration> 